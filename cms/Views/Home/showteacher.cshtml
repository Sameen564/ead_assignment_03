
@model List<cms.Models.Table>
@{
    ViewBag.Title = "showteacher";
}

<html>
<head>
    <style>
        table, th, td {
            border: 1px solid black;
        }
    </style>
</head>

<div class="limiter">
    <div class="container-table100">
        <div class="wrap-table100">
            <div class="table100">
                @if (Model.Count <= 0)
                {
                    <h2 style="color:black">No teacher exist of this category</h2>
                }
                else
                {
                    <h2 class="form-title" style="color:black">Teachers</h2>
                    <table style="width:50%">
                        <thead>
                            <tr class="table100-head">
                                <th class="column1">Teacher ID</th>
                                <th class="column1">Teacher name</th>
                                <th class="column3">Teacher contact</th>
                                <th class="column4">Teacher email</th>
                                <th class="column8">Teacher category</th>
                              


                            </tr>
                        </thead>

                        @foreach (var m in Model)
                        {
                            <tbody>
                                <tr>
                                    <td class="column1">@m.Id</td>
                                    <td class="column1">@m.username</td>
                                    <td class="column3">@m.contact</td>
                                    <td class="column4">@m.email</td>
                                    <td class="column8">@m.category</td>
                                </tr>
                            </tbody>
                        }




                    </table>
                }
                <br />
                <br />
                <br />
                <br />

            </div>

        </div>
    </div>
</html>

